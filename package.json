    "echarts-gl": "^2.0.9",
    "lunar-calendar": "^0.1.4",
    "react": "18.x",
    "react-dom": "18.x",
    "three": "^0.177.0",
    "umi": "^3.5.43",
    "umi-plugin-keep-alive": "^0.0.1-beta.30",
    "umi-request": "^1.4.0",
